{
    "version": 1,
    "disable_existing_loggers": false,
    "propagate": true,
    "formatters": {
        "default": {
            "format": "%(asctime)s - %(module)s - %(levelname)s - %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "default": {
            "formatter": "default",
            "class": "logging.StreamHandler"
        }
    },
    "loggers": {
        "": {
            "level": "INFO",
            "handlers": ["default"],
            "propagate": true
        }
    }
}
